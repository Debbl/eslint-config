{
  "name": "@debbl/eslint-config",
  "type": "module",
  "version": "3.10.2",
  "packageManager": "pnpm@10.10.0",
  "description": "Brendan Dash's ESLint config",
  "author": "Brendan Dash <me@aiwan.run> (https://github.com/Debbl/)",
  "license": "MIT",
  "homepage": "https://github.com/Debbl/eslint-config",
  "keywords": [
    "eslint-config",
    "eslint-flat-config",
    "prettier"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "stub": "tsup --format esm",
    "dev": "pnpm build && pnpm exec eslint-config-inspector --config eslint.config.js",
    "build:inspector": "pnpm build && pnpm exec eslint-config-inspector build",
    "watch": "tsup --format esm,cjs --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm run stub && eslint",
    "lint:fix": "pnpm run stub && eslint --fix",
    "release": "pnpm build && bumpp && pnpm publish"
  },
  "peerDependencies": {
    "eslint": ">=9"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint-react/eslint-plugin": "^1.42.1",
    "@eslint/markdown": "^6.3.0",
    "@next/eslint-plugin-next": "^15.2.5",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitest/eslint-plugin": "^1.1.40",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import-x": "^4.10.2",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^4.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-compiler": "latest",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.0.0",
    "eslint-plugin-yml": "^1.17.0",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "toml-eslint-parser": "^0.10.0",
    "vue-eslint-parser": "^10.1.3",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.0.2",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "bumpp": "^10.1.0",
    "eslint": "^9.24.0",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "react": "^19.1.0",
    "sucrase": "^3.35.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
