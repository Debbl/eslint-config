{
  "name": "@debbl/eslint-config",
  "type": "module",
  "version": "3.11.0",
  "packageManager": "pnpm@10.11.0",
  "description": "Brendan Dash's ESLint config",
  "author": "Brendan Dash <me@aiwan.run> (https://github.com/Debbl/)",
  "license": "MIT",
  "homepage": "https://github.com/Debbl/eslint-config",
  "keywords": [
    "eslint-config",
    "eslint-flat-config",
    "prettier"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "stub": "tsdown --format esm",
    "dev": "pnpm build && pnpm exec eslint-config-inspector --config eslint.config.js",
    "build:inspector": "pnpm build && pnpm exec eslint-config-inspector build",
    "watch": "tsdown --format esm,cjs --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm run stub && eslint",
    "lint:fix": "pnpm run stub && eslint --fix",
    "release": "pnpm build && bumpp && pnpm publish"
  },
  "peerDependencies": {
    "eslint": ">=9"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint-react/eslint-plugin": "^1.49.0",
    "@eslint/markdown": "^6.4.0",
    "@next/eslint-plugin-next": "^15.3.2",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/eslint-plugin": "^1.2.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-plugin-import-x": "^4.12.2",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react-compiler": "latest",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.1.0",
    "eslint-plugin-yml": "^1.18.0",
    "globals": "^16.1.0",
    "jsonc-eslint-parser": "^2.4.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "toml-eslint-parser": "^0.10.0",
    "vue-eslint-parser": "^10.1.3",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.0.2",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.4",
    "bumpp": "^10.1.1",
    "eslint": "^9.27.0",
    "execa": "^9.5.3",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "react": "^19.1.0",
    "sucrase": "^3.35.0",
    "tsdown": "^0.11.12",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "unrs-resolver"
    ]
  }
}
